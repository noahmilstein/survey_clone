<ol class="questions">
  <% @survey.questions.each do |question| %>
    <li>
      <%= question.prompt %>
      <ol class="answers">
        <% question.answers.each do |answer| %>
          <li><%= answer.content %></li>
        <% end %>
      </ol>
    </li>
  <% end %>
</ol>

<%= link_to "Edit", edit_survey_path(@survey) %>
